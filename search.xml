    <entry>
      <title>毕业倒计时</title>
      <link href="/2018/04/20/%E6%AF%95%E4%B8%9A%E5%80%92%E8%AE%A1%E6%97%B6/"/>
      <url>/2018/04/20/%E6%AF%95%E4%B8%9A%E5%80%92%E8%AE%A1%E6%97%B6/</url>
      <content type="html"><![CDATA[<h3 id=""><a href="#" class="headerlink" title=" "></a><div id="s" style="width:100%;height:30px;border:#ccc solid 0px; text-align:center"> </div></h3><p><br></p><h3 id="-1"><a href="#-1" class="headerlink" title=""></a><div id="f" style="width:100%;height:30px;border:#ccc solid 0px; text-align:center"></div></h3><p><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br></p><script>var a = document.getElementById("s")var f = document.getElementById("f")var weekday=new Array(7)weekday[0]="星期日"weekday[1]="星期一"weekday[2]="星期二"weekday[3]="星期三"weekday[4]="星期四"weekday[5]="星期五"weekday[6]="星期六"function time(){    var data = new Date();    var todata = new Date("2018/05/25");    //console.log("现在:"+data)    //console.log("未来:"+todata)    data.setTime(data.getTime()+250)//时间缀    var xc = todata-data    var days = Math.floor(xc/1000/60/60/24)    var h = Math.floor(xc/1000/60/60%24)    var m = Math.floor(xc/1000/60%60)    var s = Math.floor(xc/1000%24)    s=ABS(s)    s = jo(s)    //console.log(days+"天")    //console.log(h+"时")    //console.log(m+"分")    console.log(s+"秒");    var x = data.getDay();var n = 0//console.log(weekday[data.getDay()])//console.log(data.getDay())for(var i = 1 ; i<=days;i++){    //console.log(weekday[x])    x = x+1;    if(x==7)    {        x=0;        n++        //console.log(n)    }    if(x==0){        n++;        //console.log(n)    }}//console.log(days-n)var zx = days-n//console.log(s)a.innerHTML="距离毕业还有:"+days+"天"+h+"时"+m+"分"+s+"秒";f.innerHTML="毕业已经过去"+ABS(days)+"天"+ABS(h)+"时"+ABS(m)+"分"+ABS(s)+"秒";//在校时间剩余://console.log(ABS(days)+"最新")}function ABS(time){    return time*-1}setInterval(function(){    time()},60)function m2(x,n){    var a=x    for(var i=1;i<n;i++){        //console.log(i)        a = a *x    }    return a}//console.log("---------------------------------------------------------")function ABS(n){    if(n<0){        n=n*-1    }    return n}function jo(a){    if(a<10){        a = "0"+a    }    return a}</script>]]></content>
      
